<Window x:Class="Sortedlist.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sortedlist"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="358,159,0,0" VerticalAlignment="Top" Width="75" Click="btnAdd_Click"/>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="187,132,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="120" TextChanged="txtKey_TextChanged"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="187,192,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="120" TextChanged="txtValue_TextChanged"/>

    </Grid>
</Window>
